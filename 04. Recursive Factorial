using System;

namespace zad3praktikum
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            Console.WriteLine(fact(n));
        }

        static int fact(int n)
        {
            if (n == 0) return 1;
            return n * fact(n - 1);
        }
    }
}
